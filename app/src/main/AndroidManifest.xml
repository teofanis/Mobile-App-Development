    <application
        android:name=".UtilsAndInterfaces.Notifications"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activities.HomePage"
            android:launchMode="singleTop"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.RegistrationPage" />
        <activity android:name=".Activities.Dashboard" />
        <activity android:name=".Activities.TimeTable" />
        <activity android:name=".Activities.Library" />
        <activity android:name=".Activities.bookActivity" />
        <activity android:name=".Activities.Settings" />
        <activity android:name=".Activities.manage_bookings" />
        <activity android:name=".Activities.FloorPlan" />
    </application>

</manifest>